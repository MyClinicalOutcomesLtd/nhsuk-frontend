<nav role="navigation" class="app-related-nav__nav-section 
{%- if params.classes %} {{ params.classes }}{% endif %}" {%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
  <ul class="app-related-nav__list beta-hub-sibling-nav__list">
    {%- for item in params.items %}
      {%- if item.isActive %}
        <li class="beta-hub-sibling-nav__item  beta-hub-sibling-nav--current is-active" >
          {{ item.text }}
        </li>
      {%- else %}
        <li class="beta-hub-sibling-nav__item ">
          <a href="{{ item.href }}">
            {{ item.text }}
          </a>
        </li>
      {%- endif %}
    {%- endfor %}
  </ul>
</nav>


